@charset "utf-8";
* {
    margin: 0;
    padding: 0;
}
body {
    color: #212121;
    font-family: "宋体";
    font-size: 13px;
}
a {
    cursor: pointer;
    text-decoration: none;
}
img {
    border: medium none;
}
ul, li {
    list-style-type: none;
    text-decoration: none;
}
.cen_list ul p a:hover, .ultab td a:hover, .cen_list ul h4 span a:hover, .cen_list ul ul.cen_que li a:hover, .rig ul li a:hover, .rig2 ul li a:hover, .cen_leadtab td a:hover, .left ul li a:hover {
    color: #F0000B;
}
.ultab td a, .cen_list ul ul.cen_que li a, .cen_list ul h4 span a, .cen_list ul p a, .cen_leadtab td a, .rig2 ul li a, .left ul li a {
    color: #212121;
}
.container {
    background: url("../images/sh_bodybg.jpg") repeat-x scroll center top #FFFFFF;
    overflow: hidden;
    width: 100%;
}
.header {
    margin: 0 auto;
    overflow: hidden;
    width: 1000px;
}
.logo {
    display: inline;
    float: left;
    margin: 35px 0 0 10px;
    overflow: hidden;
}
.serch {
    display: inline;
    float: left;
    margin-top: 40px;
    overflow: hidden;
    padding-left: 16px;
    width: 680px;
}
.serch_tab td {
}
.serch_td01 {
    background: url("../images/sh_serchbg.jpg") repeat-x scroll center center #FFFFFF;
    border-color: #7B7B7B #B6B6B6 #B6B6B6 #7B7B7B;
    border-left: 1px solid #7B7B7B;
    border-style: solid;
    border-width: 1px;
    line-height: 32px;
    overflow: hidden;
}
.serch_text {
    background: none repeat scroll 0 0 rgba(0, 0, 0, 0);
    border: medium none;
    height: 32px;
    line-height: 32px;
    width: 445px;
}
.serch_select {
    background: none repeat scroll 0 0 rgba(0, 0, 0, 0);
    border: medium none;
    cursor: pointer;
}
.serch_select option {
    background: none repeat scroll 0 0 rgba(0, 0, 0, 0);
    border: medium none;
}
.serch_td02 {
    background: none repeat scroll 0 0 #FFFFFF;
    border: 1px solid #B6B6B6;
    height: 32px;
    line-height: 32px;
    padding-top: 2px;
    width: 38px;
}
.serch_submit {
    background: url("../images/sh_buttonbg.jpg") repeat-x scroll center center rgba(0, 0, 0, 0);
    border: 1px solid #6A6A6C;
    cursor: pointer;
    height: 32px;
    line-height: 32px;
    overflow: hidden;
    text-align: center;
    width: 81px;
}
.content {
    margin: 28px auto 0;
    overflow: hidden;
    text-align: left;
    width: 1500px;
}
.left {
    display: inline;
    float: left;
    overflow: hidden;
    width: 170px;
}
.left h3 {
    background: url("../images/sh_h3.jpg") no-repeat scroll left center rgba(0, 0, 0, 0);
    border-bottom: 1px dashed #636363;
    color: #000000;
    font-weight: bold;
    height: 32px;
    line-height: 32px;
    margin-top: 13px;
    text-indent: 10px;
    width: 100%;
}
.left ul {
    overflow: hidden;
    padding-top: 10px;
    width: 100%;
}
.left ul li {
    display: inline;
    float: left;
    height: 28px;
    line-height: 28px;
    overflow: hidden;
    text-indent: 10px;
    width: 100%;
}
.left ul li a {
    display: block;
    height: 28px;
    line-height: 28px;
}
.left ul li .selected {
    background: none repeat scroll 0 0 #EDEDED;
}
.cen {
    display: inline;
    float: left;
    margin-left: 15px;
    /*overflow: hidden;*/
    width: 570px;
}
.cen_top {
    overflow: hidden;
    width: 100%;
}
.cen_top h3 {
    display: inline;
    float: left;
    font-weight: normal;
    height: 32px;
    line-height: 32px;
    margin-top: 13px;
    overflow: hidden;
    width: 60%;
}
.cen_top ul {
    border: 1px solid #B6B6B6;
    display: inline;
    float: right;
    height: 22px;
    line-height: 22px;
    margin-top: 18px;
    overflow: hidden;
    width: 174px;
}
.cen_top ul li {
    display: inline;
    float: left;
    overflow: hidden;
    text-align: center;
    width: 50%;
}
.cen_top #myTab0 li a {
    color: #212121;
    display: block;
}
.cen_top #myTab0 li.active_down {
    background: url("../images/sh_tablih.gif") no-repeat scroll right center rgba(0, 0, 0, 0);
}
.cen_top #myTab0 li.active_up {
    background: url("../images/sh_tablih_up.gif") no-repeat scroll right center rgba(0, 0, 0, 0);
}
.cen_top #myTab0 li.normal {
    background: url("../images/sh_tabli.gif") no-repeat scroll right center rgba(0, 0, 0, 0);
}
.cen_leadtab {
    font-size: 13px;
    margin: 10px auto 0;
    overflow: hidden;
    width: 96%;
}
.cen_leadtab td {
    line-height: 24px;
    overflow: hidden;
}
.cen_leader_border {
    border: 1px solid #B6B6B6;
    display: block;
    height: 136px;
    padding: 1px;
    width: 116px;
}
.cen_leadtit {
    font-weight: bold;
}
.cen_list {
    margin: 0 auto;
   /* overflow: hidden;*/
    width: 96%;
}
.cen_list ul {
    margin: 15px auto 0;
   /* overflow: hidden;*/
   /*  padding-bottom: 10px;*/
    width: 100%;
}
.cen_list ul h3 {
    color: #207DC2;
    display: inline;
    float: left;
    font-size: 14px;
    overflow: hidden;
}
.cen_list ul h3 a, .wsbs h3 a {
    color: #207DC2;
}
.cen_list ul h4 {
    display: inline;
    float: left;
    font-size: 13px;
    font-weight: normal;
    line-height: 24px;
    overflow: hidden;
    width: 100%;
}
.cen_list ul h4 span {
    display: inline;
    overflow: hidden;
}
.cen_list ul p {
    display: inline;
    float: left;
    line-height: 18px;
    overflow: hidden;
    padding: 5px 0 0;
    width: 100%;
}
.cen_list ul li {
   /* float: left; */
    line-height: 18px;
    white-space: normal;
   /* width: 100%;*/
}
.cen_list ul li a {
    color: #2D9A56;
}
.cen_list ul ul.cen_que li {
    line-height: 26px;
}
.cen_next {
    margin-top: 10px;
    overflow: hidden;
    padding-bottom: 10px;
    width: 100%;
}
.cen_next ul {
    height: 26px;
    line-height: 24px;
    margin: 0 auto;
    overflow: hidden;
    width: 96%;
}
.cen_next ul li {
    display: inline;
    float: left;
    overflow: hidden;
    text-align: center;
}
.cen_next ul li a {
    display: block;
}
.cen_next ul li.lidown {
    margin-left: 5px;
    width: 24px;
}
.cen_next ul li.lifirst {
    margin-left: 5px;
}
.cen_next ul li.lifirst a, .cen_next ul li.lidown a {
    color: #212121;
}
.cen_next ul li.lifirst a:hover, .cen_next ul li.lidown a:hover, .cen_next ul li.liup a:hover {
    color: #F0000B;
}
.cen_next ul li.liup {
    border: 1px solid #E7ECF0;
    margin-left: 2px;
    width: 24px;
}
.cen_next ul li.liup a {
    color: #0000CC;
}
.cen_next ul li#lipage {
    margin-left: 5px;
}
.cen_list ul p a {
    /*float: right;*/
}
.rig, .rig2, .rig3 {
    display: inline;
    float: left;
    margin-left: 15px;
    overflow: hidden;
    width: 230px;
}
.rig h3 {
    color: #000000;
    font-weight: bold;
    height: 32px;
    line-height: 32px;
    margin-top: 13px;
    text-indent: 10px;
    width: 100%;
}
.rig ul {
    overflow: hidden;
    width: 100%;
}
.rig ul li {
    display: inline;
    float: left;
    line-height: 26px;
    overflow: hidden;
    text-align: center;
    width: 50%;
}
.rig ul li a {
    color: #212121;
    display: block;
    font-weight: bold;
    line-height: 26px;
}
.rig ul li a span {
    display: block;
    width: 100%;
}
.rig ul li a span.rig_imgborder {
    border: 1px solid #207DC2;
    height: 110px;
    margin: 0 auto;
    padding: 1px;
    width: 97px;
}
.rig ul li.rig_txt {
    margin-left: 10px;
    position: relative;
    text-align: left;
    width: 230px;
}
.rig ul li.rig_txt a {
    bottom: 0;
    font-weight: normal;
    position: absolute;
    right: 0;
    width: 70px;
}
.rig2 ul {
    border: 1px solid #B6B6B6;
    margin-top: 13px;
    overflow: hidden;
    width: 99%;
}
.rig2 h3 {
    background: url("../images/sh_rig04.jpg") repeat-x scroll center center rgba(0, 0, 0, 0);
    height: 30px;
    line-height: 30px;
    overflow: hidden;
    text-align: center;
    width: 100%;
}
.rig2 ul li {
    display: inline;
    float: left;
    height: 28px;
    line-height: 28px;
    overflow: hidden;
    text-align: center;
    width: 50%;
}
.rig2 ul li a {
    background: url("../images/sh_rig05.jpg") no-repeat scroll left center rgba(0, 0, 0, 0);
    padding-left: 5px;
}
#xzk {
    background: none repeat scroll 0 0 #F0F0F0;
    border: 1px solid #DAD1D2;
    overflow: hidden;
    padding-bottom: 10px;
    width: 99%;
}
.xzk_tit2_huj {
    overflow: hidden;
    width: 100%;
}
.xzk_tit2_huj h3 {
    color: #D2480A;
    font-weight: bold;
    line-height: 32px;
    margin: 0 auto;
    overflow: hidden;
    width: 94%;
}
.xzk_tit2_huj p {
    border-bottom: 1px dashed #B6B6B6;
    line-height: 22px;
    margin: 0 auto;
    text-indent: 2em;
    width: 94%;
}
.xzk_tit2_huj ul {
    width: 94%;
}
.xzk_tit2_huj ul li {
    background: none repeat scroll 0 0 rgba(0, 0, 0, 0);
    height: 29px;
    line-height: 29px;
    text-align: center;
    width: 104px;
}
#xzk .xzk_tit2_huj ul li.active {
    background: url("../images/sh_xxk01.jpg") no-repeat scroll center center rgba(0, 0, 0, 0);
}
#xzk .xzk_tit2_huj ul li.active a {
    color: #19529F;
}
#xzk .xzk_tit2_huj ul li a {
    color: #212121;
    display: block;
}
#extpagebox {
  height: 470px;
  /*left: 50%;*/
  margin-left: 220px;
  position: absolute;
  top: 160px;
  width: 500px;
  z-index: 100;
}
.Contentbox {
    background: none repeat scroll 0 0 #FFFBFC;
    margin: 0 auto;
    overflow: hidden;
    width: 94%;
}
.Contentbox ul {
    margin: 0 auto;
    overflow: hidden;
    padding: 10px 0;
    width: 90%;
}
.Contentbox ul li {
    display: inline;
    float: left;
    overflow: hidden;
    width: 50%;
}
#butn {
    margin: 0 auto;
    overflow: hidden;
    padding: 10px 0;
    text-align: center;
    width: 90%;
}
.butnbt_red {
    background: url("../images/sh_xxk02.jpg") no-repeat scroll center center rgba(0, 0, 0, 0);
    border: medium none;
    color: #FFFFFF;
    cursor: pointer;
    height: 23px;
    line-height: 23px;
    width: 75px;
}
.wsbs {
    margin-top: 15px;
}
.wsbs p {
    line-height: 24px;
}
.wsbs_table {
    padding-top: 6px;
}
.ultab {
    border-collapse: collapse;
}
.ultab td {
    padding: 0 10px 0 0;
}
.ultab td a, .cen_leadtab .cen_detail a {
    color: #0000FF;
    text-decoration: underline;
}
.result-keytip {
    font-size: 14px;
    font-weight: bold;
}
.result-intekeys {
    color: #FF0000;
    font-size: 14px;
    font-weight: bold;
    text-decoration: none;
}
.result-inteNum {
    color: #8C8C8C;
    font-size: 12px;
}
.dig-type_search {
    margin: 0 3px 0 0;
    vertical-align: middle;
}
.dig-search-type {
    display: inline-block;
    font-size: 12px;
    vertical-align: middle;
}
.con_relArticle {
    background: url("../images/list_down.gif") no-repeat scroll right top rgba(0, 0, 0, 0);
    color: #207DC2;
    cursor: pointer;
    font-size: 14px;
    line-height: 20px;
    padding-right: 20px;
}
.con_relArticleUp {
    background: url("../images/list_up.gif") no-repeat scroll right top rgba(0, 0, 0, 0);
}
.con_relArticleList {
    background: none repeat scroll 0 0 #EDEDED;
}
.con_relArticleList ul {
    margin-top: 0;
    padding: 6px 0 6px 6px;
}
.advSearch {
    color: #212121;
    display: inline-block;
    font-size: 10px;
    line-height: 14px;
    padding-left: 10px;
}
.advSearch:hover {
    color: #FF0000;
}
.footer {
    margin: 0 auto;
    padding: 20px 0;
    width: 1000px;
}
.footer p {
    line-height: 20px;
    text-align: center;
}
.footer-logo {
    background: url("../images/smallLogo.png") no-repeat scroll 2px top rgba(0, 0, 0, 0);
    padding-left: 20px;
}
